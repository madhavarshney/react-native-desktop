{
  "name": "react-native-desktop",
  "version": "0.57.8",
  "description": "A framework for building desktop apps using React and React Native",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/status-im/react-native-desktop.git"
  },
  "engines": {
    "node": ">=8.3"
  },
  "prettier": {
    "requirePragma": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "parser": "flow"
  },
  "jest": {
    "haste": {
      "defaultPlatform": "desktop",
      "providesModuleNodeModules": [
        "react-native",
        "react-native-desktop"
      ],
      "platforms": [
        "desktop"
      ]
    }
  },
  "main": "Libraries/react-native-desktop/react-native-desktop-implementation.js",
  "files": [
    "jest",
    "Libraries",
    "local-cli",
    "ReactCommon",
    "ReactQt",
    ".flowconfig",
    "LICENSE",
    "README.md",
    "ubuntu-server.js"
  ],
  "scripts": {
    "start": "node ./node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test-ci": "JEST_JUNIT_OUTPUT=\"reports/junit/js-test-results.xml\" jest --maxWorkers=2 --ci --testResultsProcessor=\"jest-junit\"",
    "flow": "flow",
    "lint": "eslint .",
    "prettier": "prettier \"./**/*.js\" --write"
  },
  "peerDependencies": {
    "react": "16.6.3",
    "react-native": "~0.57.8"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "chalk": "^1.1.1",
    "connect": "^3.6.5",
    "create-react-class": "^15.6.3",
    "fbjs": "^1.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "promise": "^7.1.1",
    "prop-types": "^15.5.8",
    "react-clone-referenced-element": "^1.0.1",
    "react-devtools-core": "^3.4.2",
    "react-timer-mixin": "^0.13.2",
    "ws": "^1.1.5",
    "yargs": "^9.0.0",
    "yeoman-environment": "^2.3.4",
    "yeoman-generator": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "async": "^2.4.0",
    "babel-eslint": "9.0.0",
    "babel-generator": "^6.26.0",
    "detox": "9.0.4",
    "eslint": "5.1.0",
    "eslint-config-fb-strict": "22.1.0",
    "eslint-config-fbjs": "2.0.1",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-flowtype": "2.43.0",
    "eslint-plugin-jest": "21.8.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.8.2",
    "eslint-plugin-react-native": "^3.2.1",
    "flow-bin": "^0.78.0",
    "jest": "23.4.1",
    "jest-junit": "5.1.0",
    "prettier": "1.13.6",
    "react": "16.6.3",
    "react-native": "~0.57.8",
    "react-native-dummy": "0.1.0",
    "react-test-renderer": "16.6.3",
    "shelljs": "^0.7.8"
  },
  "rnpm": {
    "haste": {
      "providesModuleNodeModules": [
        "react-native-desktop"
      ],
      "platforms": [
        "desktop"
      ]
    },
    "plugin": "./local-cli/index.js"
  }
}
